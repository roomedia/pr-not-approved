{
    "manifest_version": 2,
    "name": "pr-not-approved",
    "description": "Find Pull Requests that have been reviewed but not approved",
    "version": "1.0.0",

    "background": {
        "page": "/html/background.html"
    },

    "browser_action": {
        "default_popup": "/html/popup.html"
    },
    "permissions": [
        "tabs",
        "storage",
        "webRequest",
        "<all_urls>"
    ]
}
